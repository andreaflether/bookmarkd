$('#tweets-container').append('<%= j render @tweets %>');
<% if @tweets.next_page %>
  $('#paginate-load-more-button .pagination').replaceWith("<%= j will_paginate @tweets %>");
  $('#paginate-load-more-button .pagination').hide();
<% else %>
  $('#paginate-load-more-button .pagination, #load-more-tweets').remove();
<% end %>